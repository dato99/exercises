@{
    ViewData["Title"] = "Generate Country Data Files";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <button type="submit" class="btn btn-primary">Generate Country Data Files</button>
</form>

<div class="mt-4">
    @if (!string.IsNullOrEmpty(ViewBag.Message))
    {
        <p>@ViewBag.Message</p>
    }
</div>

